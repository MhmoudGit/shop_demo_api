{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "(./(.*))",
            "dest": "app/main.py"
            
        }
    ],
    "rewrites": [{ "source": "/app/(.*)", "destination": "/app/main.py" }]
}